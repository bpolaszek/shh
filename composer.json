{
  "name": "bentools/shh",
  "type": "library",
  "description": "A PHP library to encrypt/decrypt secrets using OpenSSL.",
  "keywords": [
    "symfony",
    "secret",
    "secrets",
    "openrsa",
    "openssl",
    "encrypt",
    "encryption",
    "decrypt",
    "decryption",
    "public key",
    "private key",
    "passphrase"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Beno!t POLASZEK"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0|~9.0",
    "squizlabs/php_codesniffer": "~3.0",
    "symfony/var-dumper": "~4.0|~5.0",
    "php-coveralls/php-coveralls": "~2.1",
    "phpstan/phpstan": "~0.12",
    "bentools/cartesian-product": "^1.3",
    "thecodingmachine/safe": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BenTools\\Shh\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\Shh\\Tests\\": "tests"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "suggest": {
    "bentools/shh-bundle": "Integrate Shh! into your Symfony application."
  }
}
